        <type>bool</type>
        <definition>virtual bool wxBitmapHandler::LoadFile</definition>
        <argsstring>(wxBitmap *bitmap, const wxString &amp;name, wxBitmapType type, int desiredWidth, int desiredHeight)</argsstring>
        <name>LoadFile</name>
        <param>
          <type><ref refid="classwx_bitmap" kindref="compound">wxBitmap</ref> *</type>
          <declname>bitmap</declname>
        </param>
        <param>
          <type>const <ref refid="classwx_string" kindref="compound">wxString</ref> &amp;</type>
