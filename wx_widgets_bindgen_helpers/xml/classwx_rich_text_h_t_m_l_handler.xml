<codeline><highlight class="normal">fontSizeMapping.Add(22);</highlight></codeline>
<codeline><highlight class="normal">fontSizeMapping.Add(100);</highlight></codeline>
<codeline></codeline>
<codeline><highlight class="normal">htmlHandler.SetFontSizeMapping(fontSizeMapping);</highlight></codeline>
</programlisting> </para>        </detaileddescription>
