        <definition>wxBufferedPaintDC::wxBufferedPaintDC</definition>
        <argsstring>(wxWindow *window, wxBitmap &amp;buffer, int style=wxBUFFER_CLIENT_AREA)</argsstring>
        <name>wxBufferedPaintDC</name>
        <param>
          <type><ref refid="classwx_window" kindref="compound">wxWindow</ref> *</type>
          <declname>window</declname>
        </param>
        <param>
          <type><ref refid="classwx_bitmap" kindref="compound">wxBitmap</ref> &amp;</type>
          <declname>buffer</declname>
